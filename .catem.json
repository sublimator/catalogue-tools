{
  "global_exclude": [
    "**/venv/**",
    "**/__pycache__/**",
    "**/.git/**",
    "**/.pytest_cache/**",
    "**/.mypy_cache/**"
  ],
  "files_patterns": {
    "hasher": {
      "include": [
        "CMakeLists.txt",
        "catl-hasher.cpp",
        "hasher/**/*.cpp",
        "hasher/**/*.h"
      ]
    },
    "hasher-http": {
      "include": [
        "hasher/http.cpp",
        "hasher/http.h"
      ]
    }
  }
}
